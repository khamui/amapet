{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean": "rm -rf node_modules",
    "cstart": "npm run clean && npm cache clean --force && npm i --legacy-peer-deps && npm start",
    "dev:ssr": "ng run frontend:serve-ssr",
    "serve:ssr": "node dist/frontend/server/main.js",
    "build:ssr": "ng build && ng run frontend:server",
    "prerender": "ng run frontend:prerender"
  },
  "private": true,
  "dependencies": {
    "@abacritt/angularx-social-login": "2.1.0",
    "@angular/animations": "17.0.8",
    "@angular/common": "17.0.8",
    "@angular/compiler": "17.0.8",
    "@angular/core": "17.0.8",
    "@angular/forms": "17.0.8",
    "@angular/platform-browser": "17.0.8",
    "@angular/platform-browser-dynamic": "17.0.8",
    "@angular/platform-server": "17.0.8",
    "@angular/router": "17.0.8",
    "@angular/ssr": "17.0.8",
    "express": "4.18.2",
    "jwt-decode": "4.0.0",
    "primeflex": "3.3.1",
    "primeicons": "6.0.1",
    "primeng": "17.3.1",
    "quill": "1.3.7",
    "rxjs": "7.8.1",
    "tslib": "2.6.2",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "17.0.9",
    "@angular/cli": "17.0.9",
    "@angular/compiler-cli": "17.0.8",
    "@types/express": "4.17.17",
    "@types/node": "18.18.0",
    "typescript": "5.2.2"
  }
}
